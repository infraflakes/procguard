{
  "compilerOptions": {
    "target": "ES6",
    "module": "ES6",
    "lib": ["es2017", "dom"],
    "outDir": "frontend/dist",
    "rootDir": "frontend",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["frontend/**/*.ts"]
}
